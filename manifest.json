{
  "name": "Moodle Dashboard",

  "description": "Dashboard de Anal√≠tica de Aprendizagem para dados de logs do Moodle",

  "version": "1.0",

  "homepage_url": "https://github.com/ldseinhardt/moodle-dashboard",

  "author": "Luan Einhardt <ldseinhardt@gmail.com>",

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "assets/js/content.js"
      ]
    }
  ],

  "options_page": "pages/options/index.html",

  "browser_action": {
    "default_title": "Moodle Dashboard",
    "default_icon": "assets/icons/icon-32.png",
    "default_popup": "pages/popup/index.html"
  },

  "permissions": [
    "tabs",
    "downloads",
    "storage",
    "declarativeContent",
    "http://*/*",
    "https://*/*"
  ],  

  "icons": {
    "32": "assets/icons/icon-32.png",
    "48": "assets/icons/icon-48.png"
  },

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  
  "manifest_version": 2
}